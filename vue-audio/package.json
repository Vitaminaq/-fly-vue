{
  "name": "@wefly/vue-audio",
  "version": "1.0.2",
  "description": "A simple and easy to use Audio library that relies on the VUE 3.0 framework. It supports multiple audio switches on the same page, and only one audio player exists at the same time. It supports dragging, clicking and jumping to play anywhere.",
  "author": "Vitaminaq",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue 3.0",
    "audio",
    "manage"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "main": "./dist/vue-audio.umd.js",
  "module": "./dist/vue-audio.es.js",
  "homepage": "https://github.com/Vitaminaq/plugins-vue/tree/master/vue-audio",
  "bugs": "https://github.com/Vitaminaq/plugins-vue/tree/master/vue-audio/issues",
  "typings": "./dist/types.d.ts",
  "dependencies": {
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.2.4"
  }
}
