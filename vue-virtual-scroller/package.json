{
  "name": "@wefly/vue-virtual-scroller",
  "version": "1.0.2",
  "description": "A virtual rolling component based on Vue 3.0",
  "author": "Vitaminaq",
  "license": "MIT",
  "keywords": [
    "vue",
    "scroller",
    "virtual-scroller",
    "vue 3.0",
    "list"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    }
  },
  "homepage": "https://github.com/Vitaminaq/plugins-vue/tree/master/vue-virtual-scroller",
  "bugs": "https://github.com/Vitaminaq/plugins-vue/tree/master/vue-virtual-scroller/issues",
  "typings": "./dist/types.d.ts",
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.1"
  }
}
